{
	"docdash": {
		"private": true,
		"search": true,
		"static": true,
		"typedefs": true
	},
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": "./dist/usr/lib/joshi",
		"includePattern": ".+\\.js$",
		"excludePattern": ""
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"template": "docdash/",
		"encoding": "utf8",
		"destination": "dist/usr/share/doc/joshi",
		"recurse": true,
		"verbose": true
	},
	"sourceType": "script",
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}
